<h1 style="background-color:lightblue; text-align:center;">Contacts</h1>

<% contacts = Contact.all %>

<ul>
<% for contact in contacts %>
<li><%= contact ["first_name"] %> <%= contact ["last_name"] %>
<% company = Company.find_by({"id" => contact[company_id]}) %>
, <%= contact ["email"] %>, 
<%= contact ["phone_number"] %>
<% end %>
</ul>